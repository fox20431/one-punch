{
    "$schema": "./node_modules/@angular/cli/lib/config/schema.json",
    "version": 1,
    "newProjectRoot": "projects",
    "projects": {
        "plain-punch": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/plain-punch",
            "sourceRoot": "projects/plain-punch/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/plain-punch",
                        "index": "projects/plain-punch/src/index.html",
                        "main": "projects/plain-punch/src/main.ts",
                        "polyfills": "projects/plain-punch/src/polyfills.ts",
                        "tsConfig": "projects/plain-punch/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/plain-punch/src/favicon.ico",
                            "projects/plain-punch/src/assets"
                        ],
                        "styles": [
                            "projects/plain-punch/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "fileReplacements": [
                                {
                                    "replace": "projects/plain-punch/src/environments/environment.ts",
                                    "with": "projects/plain-punch/src/environments/environment.prod.ts"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "plain-punch:build:production"
                        },
                        "development": {
                            "browserTarget": "plain-punch:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "plain-punch:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/plain-punch/src/test.ts",
                        "polyfills": "projects/plain-punch/src/polyfills.ts",
                        "tsConfig": "projects/plain-punch/tsconfig.spec.json",
                        "karmaConfig": "projects/plain-punch/karma.conf.js",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/plain-punch/src/favicon.ico",
                            "projects/plain-punch/src/assets"
                        ],
                        "styles": [
                            "projects/plain-punch/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "gloves": {
            "projectType": "library",
            "root": "projects/gloves",
            "sourceRoot": "projects/gloves/src",
            "prefix": "lib",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:ng-packagr",
                    "options": {
                        "project": "projects/gloves/ng-package.json"
                    },
                    "configurations": {
                        "production": {
                            "tsConfig": "projects/gloves/tsconfig.lib.prod.json"
                        },
                        "development": {
                            "tsConfig": "projects/gloves/tsconfig.lib.json"
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "main": "projects/gloves/src/test.ts",
                        "tsConfig": "projects/gloves/tsconfig.spec.json",
                        "karmaConfig": "projects/gloves/karma.conf.js"
                    }
                }
            }
        },
        "blog": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/blog",
            "sourceRoot": "projects/blog/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/blog",
                        "index": "projects/blog/src/index.html",
                        "main": "projects/blog/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/blog/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/blog/src/favicon.ico",
                            "projects/blog/src/assets"
                        ],
                        "styles": [
                            "projects/blog/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "options": {
                        "proxyConfig": "projects/blog/src/proxy.config.json"
                    },
                    "configurations": {
                        "production": {
                            "browserTarget": "blog:build:production"
                        },
                        "development": {
                            "browserTarget": "blog:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "blog:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/blog/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/blog/src/favicon.ico",
                            "projects/blog/src/assets"
                        ],
                        "styles": [
                            "projects/blog/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        },
        "dashboard": {
            "projectType": "application",
            "schematics": {
                "@schematics/angular:component": {
                    "style": "scss"
                }
            },
            "root": "projects/dashboard",
            "sourceRoot": "projects/dashboard/src",
            "prefix": "app",
            "architect": {
                "build": {
                    "builder": "@angular-devkit/build-angular:browser",
                    "options": {
                        "outputPath": "dist/dashboard",
                        "index": "projects/dashboard/src/index.html",
                        "main": "projects/dashboard/src/main.ts",
                        "polyfills": [
                            "zone.js"
                        ],
                        "tsConfig": "projects/dashboard/tsconfig.app.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/dashboard/src/favicon.ico",
                            "projects/dashboard/src/assets"
                        ],
                        "styles": [
                            "projects/dashboard/src/styles.scss"
                        ],
                        "scripts": []
                    },
                    "configurations": {
                        "production": {
                            "budgets": [
                                {
                                    "type": "initial",
                                    "maximumWarning": "500kb",
                                    "maximumError": "1mb"
                                },
                                {
                                    "type": "anyComponentStyle",
                                    "maximumWarning": "2kb",
                                    "maximumError": "4kb"
                                }
                            ],
                            "outputHashing": "all"
                        },
                        "development": {
                            "buildOptimizer": false,
                            "optimization": false,
                            "vendorChunk": true,
                            "extractLicenses": false,
                            "sourceMap": true,
                            "namedChunks": true
                        }
                    },
                    "defaultConfiguration": "production"
                },
                "serve": {
                    "builder": "@angular-devkit/build-angular:dev-server",
                    "configurations": {
                        "production": {
                            "browserTarget": "dashboard:build:production"
                        },
                        "development": {
                            "browserTarget": "dashboard:build:development"
                        }
                    },
                    "defaultConfiguration": "development"
                },
                "extract-i18n": {
                    "builder": "@angular-devkit/build-angular:extract-i18n",
                    "options": {
                        "browserTarget": "dashboard:build"
                    }
                },
                "test": {
                    "builder": "@angular-devkit/build-angular:karma",
                    "options": {
                        "polyfills": [
                            "zone.js",
                            "zone.js/testing"
                        ],
                        "tsConfig": "projects/dashboard/tsconfig.spec.json",
                        "inlineStyleLanguage": "scss",
                        "assets": [
                            "projects/dashboard/src/favicon.ico",
                            "projects/dashboard/src/assets"
                        ],
                        "styles": [
                            "projects/dashboard/src/styles.scss"
                        ],
                        "scripts": []
                    }
                }
            }
        }
    },
    "cli": {
        "analytics": false
    }
}